<?xml version="1.0" encoding="ISO-8859-1" ?>
<!DOCTYPE package SYSTEM "http://pear.php.net/dtd/package-1.0">
<package version="1.0">
  <name>djpt</name>
  <summary>Distributed access to Junoplay Table</summary>
  <description>Functions for accessing djpt servers</description>
  <maintainers>
    <maintainer>
      <user>mortehu</user>
      <name>Morten Hustveit</name>
      <email>morten@rashbox.org</email>
      <role>lead</role>
    </maintainer>
  </maintainers>
  <release>
    <version>1.0.1</version>
    <date>2008-05-09</date>
    <license>GPL</license>
    <state>stable</state>
    <notes>No notes.</notes>
    <deps>
      <dep type="php" rel="ge" version="4.3.0"/>
    </deps>
    <configureoptions>
      <configureoption name="enable-djpt" default="yes" prompt="Enable djpt?"/>
    </configureoptions>
    <filelist>
      <file role="src" name="Makefile.in"/>
      <file role="src" name="configure"/>
      <file role="src" name="config.m4"/>
      <file role="src" name="php-djpt.c"/>
      <file role="src" name="php-djpt.h"/>
    </filelist>
  </release>
</package>
